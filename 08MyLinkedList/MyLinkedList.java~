public class MyLinkedList{

    public class LNode{
	private int value;
	private LNode next;
	
	public LNode(int data){
	    value=data;
	}

	public void set(int ans){
	    value=ans;
	}

	public int get(){
	    return value;
	}

	public LNode getNext(){
	    return next;
	}

	public void setNext(int ans){
	    next=new LNode(ans);
	}
    }
